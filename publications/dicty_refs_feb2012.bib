@ARTICLE{Sultana2012,
  author = {Sultana, H. and Neelakanta, G. and Rivero, F. and Blau-Wasser, R.
	and Schleicher, M. and Noegel, A. A.},
  title = {Ectopic expression of Cyclase associated protein CAP restores the
